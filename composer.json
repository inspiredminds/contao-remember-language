{
    "name": "inspiredminds/contao-remember-language",
    "description": "Contao 4 extension to redirect to a language saved in a cookie.",
    "keywords": ["contao", "cookie", "language", "remember"],
    "type": "contao-bundle",
    "homepage": "https://github.com/inspiredminds/contao-remember-language",
    "license": "LGPL-3.0-or-later",
    "authors": [
        {
            "name": "Fritz Michael Gschwantner",
            "homepage": "http://www.inspiredminds.at",
            "email": "fmg@inspiredminds.at",
            "role": "Developer"
        }
    ],
    "support": {
        "issues": "https://github.com/inspiredminds/contao-remember-language/issues",
        "source": "https://github.com/inspiredminds/contao-remember-language",
        "forum": "https://community.contao.org/de"
    },
    "funding": [
        {
            "type": "github",
            "url": "https://github.com/sponsors/fritzmg"
        }
    ],
    "require": {
        "php": ">=7.1",
        "contao/core-bundle": "^4.9",
        "symfony/config": "^4.4",
        "symfony/dependency-injection": "^4.4",
        "symfony/event-dispatcher": "^4.4",
        "symfony/http-foundation": "^4.4",
        "symfony/http-kernel": "^4.4",
        "symfony/routing": "^4.4"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.16"
    },
    "autoload": {
        "psr-4": {
            "InspiredMinds\\ContaoRememberLanguage\\": "src/"
        }
    },
    "extra": {
        "contao-manager-plugin": "InspiredMinds\\ContaoRememberLanguage\\ContaoManager\\Plugin"
    }
}
